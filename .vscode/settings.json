{
  "files.exclude": {
    ".directory": true,
    "dist": false,
    "out": false
  },
  "search.exclude": {
    "docs": true,
    "dist": true,
    "out": true
  },
  /*
  |-···――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――···-|
  */
  "workbench.colorTheme": "Pop N' Lock Theme by Luxcium ✨ Alexis's Black ⛷",
  "workbench.iconTheme": "material-icon-theme",
  "window.zoomLevel": 2,
  "editor.fontFamily": "'VictorMono Nerd Font',Victor Mono, Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 20,
  "editor.fontWeight": "500",
  "terminal.integrated.fontFamily": "'TerminessTTF Nerd Font Mono', 'Anonymice NF', '3270Narrow NF', mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "600",
  "terminal.integrated.fontWeightBold": "bold",
  /*
  |-···――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――···-|
  ―― FORMAT BEHAVIOUR
  */
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": true,
  "editor.suggest.showFiles": true,
  "editor.suggest.showFolders": true,
  //                                                                          ―― ~
  // ―― FILES BEHAVIOUR
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  //                                                                          ―― ~
  // ―― JAVASCRIPT BEHAVIOUR
  "javascript.format.enable": true,
  "javascript.format.semicolons": "insert",
  "javascript.suggest.paths": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  //                                                                          ―― ~
  // ―― TYPESCRIPT BEHAVIOUR
  "typescript.format.enable": true,
  "typescript.format.semicolons": "insert",
  "typescript.suggest.paths": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  //                                                                          ―― ~
  // ―― CODEACTIONSONSAVE BEHAVIOUR
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.organizeImports",
    "source.fixAll.eslint",
    "source.fixAll",
  ],
  /*
  |-···――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――···-|
  ―― PATH-INTELLISENSE
  */
  "path-intellisense.extensionOnImport": false,
  "path-intellisense.absolutePathToWorkspace": false,
  "path-intellisense.autoSlashAfterDirectory": false,
  "path-intellisense.showHiddenFiles": true,
  /*
  |-···――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――···-|
  ―― PRETTIER
  */
  "prettier.enable": true,
  "prettier.requireConfig": true,
  "prettier.trailingComma": "all",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  /*
  |-···――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――···-|
  ―― ESLINT
  */
  "eslint.quiet": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.packageManager": "yarn",
  /*
  |-···――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――···-|
  ―― ERRORLENS
  */
  "errorLens.annotationPrefix": [
    "🍒 ~~> ", // ―1 error
    "🧩 ==> ", // ―2 warning
    "✨ -> ", // ―3 info
    "🎯 +> ", // ―4 hint
  ],
  "errorLens.enabledDiagnosticLevels": [
    "error", //    0- #F36F
    "warning", //  0- #CF3F
    "info", //     0- #FB0F
    "hint", //     0- #39CF
  ],
  "errorLens.exclude": [
    "may be converted to an ES6 module",
    "is declared but its value is never read"
  ],
  "errorLens.addAnnotationTextPrefixes": true,
  "errorLens.addNumberOfDiagnostics": true,
  "errorLens.borderRadius": "10px",
  "errorLens.delay": 0,
  "errorLens.errorGutterIconPath": "",
  "errorLens.followCursor": "allLines",
  "errorLens.followCursorMore": 0,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.gutterIconsFollowCursorOverride": true,
  "errorLens.gutterIconSize": "80%",
  "errorLens.infoGutterIconPath": "",
  "errorLens.light": {},
  "errorLens.margin": "0.75rem",
  "errorLens.messageEnabled": true,
  "errorLens.onSave": false,
  "errorLens.onSaveTimeout": 1000,
  "errorLens.padding": "0rem 0.5rem",
  "errorLens.scrollbarHackEnabled": false,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.statusBarMessageType": "closestProblem",
  "errorLens.warningGutterIconPath": "",
  "errorLens.errorGutterIconColor": "#F36F",
  "errorLens.warningGutterIconColor": "#BF2",
  "errorLens.infoGutterIconColor": "#4AEF",
  "workbench.colorCustomizations": {
    "errorLens.errorForeground": "#F36F",
    "errorLens.errorBackground": "#F364",
    "errorLens.errorMessageBackground": "#F364",
