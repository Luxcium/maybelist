{
  "files.exclude": {
    "dist": false,
    ".directory": true
  },

  "search.exclude": {
    "dist": true
  },

  "errorLens.annotationPrefix": ["🍒 ~> ", "⚠️ => ", "🧩 ", "🎯 -> "],
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.organizeImports",
    "source.fixAll.eslint"
  ],
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },

  "eslint.codeAction.disableRuleComment": {
    "location": "separateLine",
    "enable": true
  },

  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },

  "material-icon-theme.files.associations": {
    "index.ts": "Raml",
    "index.js": "json",
  },

  "workbench.colorCustomizations": {
    "errorLens.errorForeground": "#F36F",
    "errorLens.errorMessageBackground": "#F366",
    "errorLens.errorBackground": "#F363",
    "errorLens.warningForeground": "#CF3F",
    "errorLens.warningMessageBackground": "#CF36",
    "errorLens.warningBackground": "#CF33",
    "errorLens.hintForeground": "#39CF",
    "errorLens.hintMessageBackground": "#39C6",
    "errorLens.hintBackground": "#39C3",
    "errorLens.infoForeground": "#FB0F",
    "errorLens.infoMessageBackground": "#FB06",
    "errorLens.infoBackground": "#FB03"
  },

  "better-comments.tags": [
    {
      // WARNING:
      "tag": "WARNING:",
      "color": "#FD3E",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    {
      // !!
      "tag": "!!",
      "color": "#FF2D00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    {
      // ?
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    {
      // $
      "tag": "$",
      "color": "#f48e41",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    {
      // IIFE
      "tag": "IIFE",
      "color": "#FF3D",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    {
      // #region
      "tag": "#region",
      "color": "#555",
      "strikethrough": false,
      "backgroundColor": "#FF3D"
    },

    {
      // #endregion
      "tag": "#endregion",
      "color": "#222",
      "strikethrough": false,
      "backgroundColor": "#F15E"
    },

    {
      // #
      "tag": "#",
      "color": "#4AE",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },

    {
