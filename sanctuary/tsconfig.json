    "module": "commonjs",
    "noEmit": true,
    "allowJs": true,
    "checkJs": false,
    "rootDir": ".",
    "outDir": "out",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnusedLabels": true,
    "strict": false
