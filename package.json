{
  "name": "maybelist",
  "version": "0.0.0",
  "description": "MaybeList loose interpretation of a monadic array type or something like that",
  "keywords": [],
  "license": "ISC",
  "author": "Benjamin Vincent Kasapoglu @Luxcium",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run fix && npm run test || exit 7",
    "build": "mkdir -p \"./out/performance\" && tsc --noEmit false --generateCpuProfile \"./out/performance\" || exit 1",
    "precibuild": "npm run build || exit 2",
    "cibuild": "tsc --build \"tsconfig.src-build.json\" || exit 1",
    "postcibuild": "[ \"${ZE160DDEEx}\" = \"1050D673\" ] && (coveralls < coverage/lcov.info); exit 0",
    "coverage": "jest --verbose false --silent true --coverage || exit 1",
    "lint": "eslint \"./src/**/*.*\" ---fix-dry-run || exit 1",
    "pretty": "prettier --config \"./.prettierrc\" --loglevel log -c \"./src/**/*.*\"|| exit 1",
    "lint:fix": "eslint \"./src/**/*.*\" --fix || exit 1",
    "pretty:fix": "prettier --config \"./.prettierrc\" --loglevel log -c --write \"./src/**/*.*\" || exit 1",
    "pretty:fix:all": "prettier --config \"./.prettierrc\" --loglevel log -c --write \"**/*.*\" || exit 1",
    "pretty:all": "prettier --config \"./.prettierrc\" --loglevel log -c \"**/*.*\" || exit 1",
    "prefix": "npx eslint --print-config src/index.ts | npx eslint-config-prettier-check || exit 1",
    "fix": "npm run lint:fix && npm run pretty:fix || exit 4",
    "test": "jest --verbose true || exit 1",
    "precheck": "npm run fix || exit 5",
    "check": "npm run test && tsc --noEmit --listFiles ||exit 3",
    "postcheck": "npm run coverage || exit 2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@types/chai": "^4.2.14",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "chai": "^4.2.0",
    "chai-jest-mocks": "^1.2.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "prettier": "2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
