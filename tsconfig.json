{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "moduleResolution": "Node",
    /*                                                                        */
    "rootDir": "./",
    "noEmit": false,
    "outDir": "./build",
    "declaration": true,
    "declarationDir": "./build/typings",
    "incremental": true,
    "tsBuildInfoFile": "./build/ts-build-info.lib",
    /*                                                                        */
    "composite": true,
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "assumeChangesOnlyAffectDirectDependencies": false,
    "useDefineForClassFields": true,
    /*                                                                        */
    "removeComments": true,
    "esModuleInterop": false,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf",
    /*                                                                        */
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "declarationMap": true,
    "isolatedModules": true,
    "generateCpuProfile": "./out/cpu-info.cpuprofile",
    /*                                                                        */
    "skipLibCheck": false,
    "noEmitHelpers": false,
    "importHelpers": true,
    "importsNotUsedAsValues": "remove",
    /*                                                                        */
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true,
    /*                                                                        */
    "pretty": true,
    "watch": false,
    /*                                                                        */
    "listFiles": false,
    "diagnostics": true,
    "traceResolution": false,
    "listEmittedFiles": false,
    "extendedDiagnostics": true,
    /*                                                                        */
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    /*                                                                        */
    "lib": [
      "esnext"
    ],
    "types": [
      "node",
      "jest"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    /* ALWAYS TRUE STRICT TYPE-CHECKING, ADDITIONAL CHECKS MUST ALWAYS BE TRUE*/
    /* ALWAYS TRUE */ "strict": true /*                         ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "alwaysStrict": true /*                   ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "noImplicitAny": true /*                  ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "noImplicitThis": true /*                 ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "strictBindCallApply": true /*            ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "strictFunctionTypes": true /*            ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "strictNullChecks": true /*               ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "strictPropertyInitialization": true /*   ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "noFallthroughCasesInSwitch": true /*     ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "noImplicitReturns": true /*              ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "noUnusedLocals": true /*                 ALWAYS TRUE  */,
    /* ALWAYS TRUE */ "noUnusedParameters": true /*             ALWAYS TRUE  */,
    /* ALWAYS TRUE STRICT TYPE-CHECKING, ADDITIONAL CHECKS MUST ALWAYS BE TRUE*/
    /*                                                                        */
    "disableReferencedProjectLoad": true,
    "disableSolutionSearching": false,
    "disableSourceOfProjectReferenceRedirect": false,
    "emitBOM": false,
    "emitDeclarationOnly": false,
    "listFilesOnly": false,
    "noEmitOnError": false,
    "noImplicitUseStrict": false,
    "noLib": false,
    "noResolve": false,
    "noStrictGenericChecks": false,
    "noUncheckedIndexedAccess": false,
    "preserveConstEnums": false,
    "preserveSymlinks": false,
    "stripInternal": false,
    // "baseUrl": "./"
    // "charset": "",
    // "disableSizeLimit": false,
    // "fallbackPolling": "dynamicPriorityPolling",
    // "jsx": "preserve",
    // "jsxFactory": "React.createElement",
    // "jsxFragmentFactory": "React.Fragment",
    // "jsxImportSource": "react",
    // "keyofStringsOnly": false,
    // "mapRoot": "",
    // "maxNodeModuleJsDepth": 0,
    // "noErrorTruncation": false,
    // "outFile": "",
    // "paths": {},
    // "plugins": [],
    // "preserveWatchOutput": false,
    // "reactNamespace": "React",
    // "rootDirs": [],
    // "skipDefaultLibCheck": false,
    // "sourceRoot": "",
    // "suppressExcessPropertyErrors": false,
    // "suppressImplicitAnyIndexErrors": false,
    // "watchDirectory": "dynamicPriorityPolling",
    // "watchFile": "dynamicPriorityPolling",
  },
  "compileOnSave": true,
  //  "extends": "",
  // "files": [],
  // "references": [],
  // "ts-node": {},
  // "typeAcquisition": {},
  "include": [
    "./src/**/*.ts",
    "./test/**/*.**.ts",
    "backup/types.bak.ts",
    "tests/functor.test.ts",
  ],
  "exclude": [
    "templates/either.ts",
    "templates/maybe.ts",
    "./src/core/maybelist/maybelist3.ts",
    "./src/core/maybelist/maybelist4.ts",
    "./.git/**/**.*",
    "./.github/**/*.*",
    "./.vscode/**/*.*",
    "./dist/**/*.*",
    "./out/**/*.*",
    "./config/**/*.*",
    "./node_modules/**/*.*",
    "./scripts/**/*.*"
    // "**/*.spec.*",
    // "**/*.test.*",
  ]
}
